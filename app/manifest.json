{
  "manifest_version": 2,
  "name": "Office Login Directly",
  "description": "Directly login into eoffice from google chrome extension",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "js": ["jquery.min.js", "app.js", "bootstrap.min.js"],
    "matches": ["http://*/*", "https://*/*"]
  }],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://api.openweathermap.org/*",
    "http://192.168.152.236/",
    "storage"
  ]
}